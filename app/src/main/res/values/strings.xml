    <string name="pref_auto_unmute_title">Auto Unmute</string>
    
    <string name="pref_auto_unmute_default_volume_title">Default Volume</string>
    <string name="pref_auto_unmute_default_volume_summary">Volume to use after being manually muted.</string>
    
    <string name="pref_auto_unmute_show_ui_title">Show Volume Control</string>
    <string name="pref_auto_unmute_show_ui_summary_on">Show volume control when auto unmuting.</string>
    <string name="pref_auto_unmute_show_ui_summary_off">Don\'t show anything when auto unmuting.</string>
    
